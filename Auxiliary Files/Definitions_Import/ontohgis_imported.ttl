@prefix : <http://purl.org/ontohgis#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix oc: <http://ontologies.makolab.com/oc/> .
@prefix obo: <http://purl.obolibrary.org/obo/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://purl.org/ontohgis> .

<http://purl.org/ontohgis> rdf:type owl:Ontology ;
                            owl:imports <http://www.opengis.net/ont/geosparql> ,
                                        oc:oc.ttl ,
                                        <http://purl.obolibrary.org/obo/bfo/2.0/bfo.owl> ,
                                        <http://purl.obolibrary.org/obo/iao/2015-02-23/iao.owl> ;
                            rdfs:comment "Mapped onto DBpedia ontology (2014)"@en ;
                            owl:versionInfo 3.0 ;
                            rdfs:comment "Mapped onto Geonames feature codes"@en ,
                                         "Ontologia powstała w ramach projektu 'Ontologiczne podstawy budowy historycznych systemów informacji geograficznej' (2bH 15 0216 83) finansowanego przez Narodowy Programu Rozwoju Humanistyki 2015 r., IV edycja, moduł \"Rozwój\" 2 b."@pl ,
                                         "Ontology was developed thanks to the Ontological Foundations for Building Historical Geoinformation Systems (2bH 15 0216 83) grant funded by National Programme for the Development of Humanities (http://nprh.org/)."@en ,
                                         "Mapped onto types of GOV ontology (http://gov.genealogy.net/)"@en ,
                                         "Mapped onto linkedgeodata.org ontology (2014-09-09)"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/ontohgis#hasExternalIdentifier
:hasExternalIdentifier rdf:type owl:AnnotationProperty ;
                       rdfs:label "(OWL) class has foreign identifier"@en ,
                                  "(klasa OWL) ma obcy identyfikator"@pl ;
                       rdfs:range xsd:integer .


###  http://purl.org/ontohgis#isDefinedByInOntology
:isDefinedByInOntology rdf:type owl:AnnotationProperty ;
                       rdfs:label "is defined by in this ontology as"@en ,
                                  "jest zdefiniowane w tej ontologii jako"@pl ;
                       rdfs:subPropertyOf rdfs:isDefinedBy .


###  http://purl.org/ontohgis#isPartOfPartition
:isPartOfPartition rdf:type owl:AnnotationProperty ;
                   rdfs:label "is a member of logical partition"@en ,
                              "jest częścią podziału"@pl ;
                   rdfs:range <http://www.w3.org/2004/02/skos/core#ConceptScheme> .


###  http://purl.org/ontohgis#isValidAfter
:isValidAfter rdf:type owl:AnnotationProperty ;
              rdfs:label "is in use after"@en ,
                         "jest w użyciu po"@pl ;
              rdfs:subPropertyOf :isValidAt .


###  http://purl.org/ontohgis#isValidAround
:isValidAround rdf:type owl:AnnotationProperty ;
               rdfs:label "is in use around"@en ,
                          "jest w użyciu około"@pl ;
               rdfs:subPropertyOf :isValidAt .


###  http://purl.org/ontohgis#isValidAt
:isValidAt rdf:type owl:AnnotationProperty ;
           rdfs:comment "Relacja służy do określania czasu powstania i/lub wykorzystania źródła historycznego."@pl ;
           rdfs:label "(source) is in use at"@en ,
                      "(źródło) jest w użyciu (w)"@pl ;
           rdfs:range xsd:dateTime .


###  http://purl.org/ontohgis#isValidBefore
:isValidBefore rdf:type owl:AnnotationProperty ;
               rdfs:label "is in use before"@en ,
                          "jest w użyciu przed"@pl ;
               rdfs:subPropertyOf :isValidAt .


###  http://purl.org/ontohgis#isValidThroughout
:isValidThroughout rdf:type owl:AnnotationProperty ;
                   rdfs:comment "Relacja służy do ustalenia granic czasowych i przestrzennych poprawnego użycia kategorii."@pl ;
                   rdfs:label "is is use throughout"@en ,
                              "jest w użyciu (na)"@pl ;
                   rdfs:range :object_204 .


###  http://purl.org/ontohgis/hasOntologyDevelopmentComment
<http://purl.org/ontohgis/hasOntologyDevelopmentComment> rdf:type owl:AnnotationProperty ;
                                                         rdfs:label "has comment from ontology development team"@en ,
                                                                    "ma komentarz od zespołu tworzącego ontologię"@pl ;
                                                         rdfs:subPropertyOf rdfs:comment .


###  http://purl.org/ontohgis/is_mentioned_in_source
<http://purl.org/ontohgis/is_mentioned_in_source> rdf:type owl:AnnotationProperty ;
                                                  rdfs:label "is mentioned in source"@en ,
                                                             "jest wspomniany w źródle"@pl ;
                                                  rdfs:range <http://purl.org/ontohgis/source> .


#################################################################
#    Object Properties
#################################################################

###  http://ontologies.makolab.com/oc/hasManifestation
oc:hasManifestation rdf:type owl:ObjectProperty .


###  http://ontologies.makolab.com/oc/hasPredecessorEntityManifestation
oc:hasPredecessorEntityManifestation rdf:type owl:ObjectProperty .


###  http://ontologies.makolab.com/oc/isManifestationOf
oc:isManifestationOf rdf:type owl:ObjectProperty .


###  http://ontologies.makolab.com/oc/isPredecessorEntityManifestationOf
oc:isPredecessorEntityManifestationOf rdf:type owl:ObjectProperty ,
                                               owl:TransitiveProperty .


###  http://purl.obolibrary.org/obo/IAO_0000136
obo:IAO_0000136 rdf:type owl:ObjectProperty ;
                owl:inverseOf :InverseIAO_0000136 .


###  http://purl.obolibrary.org/obo/IAO_0000219
obo:IAO_0000219 rdf:type owl:ObjectProperty .


###  http://purl.org/ontohgis#InverseIAO_0000136
:InverseIAO_0000136 rdf:type owl:ObjectProperty ;
                    rdfs:label "about in"@en .


###  http://purl.org/ontohgis#administersOver
:administersOver rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 rdf:type owl:FunctionalProperty ;
                 rdfs:domain :object_203 ;
                 rdfs:range :object_204 ;
                 <http://purl.org/ontohgis/hasOntologyDevelopmentComment> "Jeżeli organ administracji musi być zlokalizowany na obszarze swojej właściwości, relacja ta jest podrelacją isPartOf."@en ;
                 rdfs:label "administers over"@en ,
                            "ma właściwość w obszarze"@pl .


###  http://purl.org/ontohgis#administrativeUnitAdministerAdministrativeUnit
:administrativeUnitAdministerAdministrativeUnit rdf:type owl:ObjectProperty ;
                                                rdfs:subPropertyOf :featureContainsFeature ;
                                                rdfs:domain :object_200 ;
                                                rdfs:range :object_200 ;
                                                rdfs:label "administrative unit administers administrative unit"@en .


###  http://purl.org/ontohgis#administrativeUnitAdministerSettlementPart
:administrativeUnitAdministerSettlementPart rdf:type owl:ObjectProperty ;
                                            rdfs:subPropertyOf :featureContainsFeature ;
                                            owl:inverseOf :settlementIsPartOfAdministrativeUnit ;
                                            rdfs:domain :object_200 ;
                                            rdfs:range :object_40 ;
                                            rdfs:label "administrative unit administers settlement unit"@en .


###  http://purl.org/ontohgis#administrativeUnitIsAdministeredByAdministrativeUnit
:administrativeUnitIsAdministeredByAdministrativeUnit rdf:type owl:ObjectProperty ;
                                                      rdfs:subPropertyOf :featureIsContainedInFeature ;
                                                      owl:inverseOf :administrativeUnitIsAdministeredByAdministrativeUnit ;
                                                      rdfs:label "administrative unit is administered by administrative unit"@en .


###  http://purl.org/ontohgis#featureContainsFeature
:featureContainsFeature rdf:type owl:ObjectProperty ;
                        rdfs:domain <http://www.opengis.net/ont/geosparql#Feature> ;
                        rdfs:range <http://www.opengis.net/ont/geosparql#Feature> ;
                        owl:propertyChainAxiom ( <http://www.opengis.net/ont/geosparql#hasGeometry>
                                                 <http://www.opengis.net/ont/geosparql#ehContains>
                                                 :isGeometryOf
                                               ) ;
                        rdfs:label "feature contains feature"@en .


###  http://purl.org/ontohgis#featureIsContainedInFeature
:featureIsContainedInFeature rdf:type owl:ObjectProperty ;
                             rdfs:label "feature is contained in feature"@en .


###  http://purl.org/ontohgis#hasFunction
:hasFunction rdf:type owl:ObjectProperty ;
             owl:inverseOf <http://purl.org/ontohgis/isFunctionOf> ;
             rdfs:label "has function"@en ,
                        "ma funkcję"@pl .


###  http://purl.org/ontohgis#hasImproperFunction
:hasImproperFunction rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :hasFunction ;
                     owl:propertyDisjointWith :hasProperFunction ;
                     rdfs:label "has improper function"@en ,
                                "ma funkcję niewłaściwą"@pl ;
                     rdfs:seeAlso "http://dx.doi.org/10.1080/13869790410001694462"^^xsd:anyURI .


###  http://purl.org/ontohgis#hasProperFunction
:hasProperFunction rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :hasFunction ;
                   owl:inverseOf :isProperFunctionOf ;
                   rdfs:label "has proper function"@en ,
                              "ma funkcję właściwą"@pl ;
                   rdfs:seeAlso "http://dx.doi.org/10.1080/13869790410001694462"^^xsd:anyURI .


###  http://purl.org/ontohgis#isAdditionalyDenotedBy
:isAdditionalyDenotedBy rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf :InverseIAO_0000136 ;
                        rdfs:domain :object_40 ;
                        rdfs:range :object_31 ;
                        owl:propertyDisjointWith :isDefinedBy ;
                        rdfs:label "is (additionaly) denoted by"@en ,
                                   "jest (dodatkowo) opisana przez"@pl .


###  http://purl.org/ontohgis#isDefinedBy
:isDefinedBy rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :InverseIAO_0000136 ;
             rdf:type owl:FunctionalProperty ;
             rdfs:domain :object_40 ;
             rdfs:range :object_31 ;
             rdfs:label "is defined by"@en ,
                        "jest zdefiniowana przez"@pl .


###  http://purl.org/ontohgis#isGeometryOf
:isGeometryOf rdf:type owl:ObjectProperty ;
              owl:inverseOf <http://www.opengis.net/ont/geosparql#hasGeometry> ;
              rdfs:label "jest charakterystyką przestrzenną"@pl .


###  http://purl.org/ontohgis#isProperFunctionOf
:isProperFunctionOf rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf <http://purl.org/ontohgis/isFunctionOf> ;
                    rdfs:label "jest funkcją właściwą"@pl .


###  http://purl.org/ontohgis#settlementIsPartOfAdministrativeUnit
:settlementIsPartOfAdministrativeUnit rdf:type owl:ObjectProperty ;
                                      rdfs:subPropertyOf :featureIsContainedInFeature ;
                                      rdf:type owl:FunctionalProperty ;
                                      rdfs:label "settlement unit is administered by administrative unit"@en .


###  http://purl.org/ontohgis/hasBoundary
<http://purl.org/ontohgis/hasBoundary> rdf:type owl:ObjectProperty ;
                                       rdfs:subPropertyOf obo:BFO_0000051 ;
                                       owl:inverseOf <http://purl.org/ontohgis/isBoundaryOf> ;
                                       rdfs:range obo:BFO_0000140 ;
                                       rdfs:label "has boundary"@en ,
                                                  "ma granicę"@pl .


###  http://purl.org/ontohgis/isBoundaryOf
<http://purl.org/ontohgis/isBoundaryOf> rdf:type owl:ObjectProperty ;
                                        rdfs:subPropertyOf obo:BFO_0000050 ;
                                        rdfs:domain obo:BFO_0000140 ;
                                        rdfs:label "is boundary of"@en ,
                                                   "jest granicą"@pl .


###  http://purl.org/ontohgis/isFunctionOf
<http://purl.org/ontohgis/isFunctionOf> rdf:type owl:ObjectProperty ;
                                        rdfs:domain obo:BFO_0000034 ;
                                        rdfs:label "is function of"@en ,
                                                   "jest funkcją"@pl .


###  http://purl.org/ontohgis/settlementHasSettlementPart
<http://purl.org/ontohgis/settlementHasSettlementPart> rdf:type owl:ObjectProperty ;
                                                       rdfs:subPropertyOf :featureContainsFeature ;
                                                       owl:inverseOf <http://purl.org/ontohgis/settlementIsPartOfSettlement> ;
                                                       rdfs:domain :object_40 ;
                                                       rdfs:range :object_40 ;
                                                       owl:propertyDisjointWith <http://purl.org/ontohgis/settlementIsPartOfSettlement> ;
                                                       rdfs:label "jednostka osadnicza ma część (jednostkę osadniczą)"@pl ,
                                                                  "settlement unit has part (settlement unit)"@en .


###  http://purl.org/ontohgis/settlementIsPartOfSettlement
<http://purl.org/ontohgis/settlementIsPartOfSettlement> rdf:type owl:ObjectProperty ;
                                                        rdfs:subPropertyOf :featureIsContainedInFeature ;
                                                        rdf:type owl:FunctionalProperty ;
                                                        rdfs:label "jednostka osadnicza jest częścią jednostki osadniczej"@pl ,
                                                                   "settlement unit is part of settlement unit"@en .


###  http://www.geonames.org/ontology#featureCode
<http://www.geonames.org/ontology#featureCode> rdf:type owl:ObjectProperty .


###  http://www.opengis.net/ont/geosparql#hasGeometry
<http://www.opengis.net/ont/geosparql#hasGeometry> rdfs:label "ma charakterystykę przestrzenną"@pl .


#################################################################
#    Data properties
#################################################################

###  http://ontologies.makolab.com/oc/hasBeginning
oc:hasBeginning rdf:type owl:DatatypeProperty .


###  http://ontologies.makolab.com/oc/hasEnd
oc:hasEnd rdf:type owl:DatatypeProperty .


###  http://purl.org/ontohgis#individualHasExternalIdentifier
:individualHasExternalIdentifier rdf:type owl:DatatypeProperty ,
                                          owl:FunctionalProperty ;
                                 rdfs:range xsd:integer ;
                                 rdfs:label "(individual) has external identifier"@en ,
                                            "(indywiduum) ma obcy identyfikator"@pl .


###  http://purl.org/ontohgis#isPublishedAfter
:isPublishedAfter rdf:type owl:DatatypeProperty ,
                           owl:FunctionalProperty ;
                  rdfs:domain <http://purl.org/ontohgis/source> ;
                  rdfs:range xsd:dateTime ;
                  rdfs:label "is published after"@en ,
                             "jest opublikowane po"@pl .


###  http://purl.org/ontohgis#isPublishedBefore
:isPublishedBefore rdf:type owl:DatatypeProperty ,
                            owl:FunctionalProperty ;
                   rdfs:domain <http://purl.org/ontohgis/source> ;
                   rdfs:range xsd:dateTime ;
                   rdfs:label "is published before"@en ,
                              "jest opublikowane przed"@pl .


#################################################################
#    Classes
#################################################################

###  http://ontologies.makolab.com/oc/NonVariableEntity
oc:NonVariableEntity rdfs:subClassOf obo:BFO_0000002 .


###  http://ontologies.makolab.com/oc/VariableEntity
oc:VariableEntity rdfs:subClassOf obo:BFO_0000002 .


###  http://purl.org/ontohgis#object_10
:object_10 rdf:type owl:Class ;
           rdfs:subClassOf obo:BFO_0000040 ,
                           <http://www.opengis.net/ont/geosparql#Feature> ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :hasProperFunction ;
                             owl:someValuesFrom owl:Thing
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://www.opengis.net/ont/geosparql#hasGeometry> ;
                             owl:someValuesFrom owl:Thing
                           ] ;
           rdfs:label "artefact"@en ,
                      "artefakt"@pl ,
                      "artifact"@en .


###  http://purl.org/ontohgis#object_100
:object_100 rdf:type owl:Class ;
            owl:equivalentClass [ owl:intersectionOf ( :object_40
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty oc:hasPredecessorEntityManifestation ;
                                                         owl:someValuesFrom :object_32
                                                       ]
                                                     ) ;
                                  rdf:type owl:Class
                                ] ;
            rdfs:subClassOf :object_40 ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty <http://www.geonames.org/ontology#featureCode> ;
                              owl:hasValue <http://www.geonames.org/ontology#P.PPLQ>
                            ] ;
            rdfs:label "była miejscowość"@pl ,
                       "former dwelling"@en ;
            rdfs:seeAlso "http://dbpedia.org/ontology/HistoricalSettlement"^^xsd:anyURI .


###  http://purl.org/ontohgis#object_1000
:object_1000 rdf:type owl:Class ;
             rdfs:subClassOf :object_32 ;
             :isPartOfPartition :object_704 ;
             rdfs:comment "Miejscowość jest ogólnie dostępna jeżeli nie jest przeznaczona do zamieszkiwania przez określoną grupę osób."@pl ;
             rdfs:label "ogólnie dostępna miejscowość"@pl ,
                        "universal dwelling"@en .


###  http://purl.org/ontohgis#object_1001
:object_1001 rdf:type owl:Class ;
             rdfs:subClassOf :object_32 ;
             :isPartOfPartition :object_704 ;
             rdfs:comment "Miejscowość jest dedykowana jeżeli jest przeznaczona do zamieszkiwania przez określoną grupę osób."@pl ;
             rdfs:label "dedykowana miejscowość"@pl ,
                        "special dwelling"@pl .


###  http://purl.org/ontohgis#object_12
:object_12 rdf:type owl:Class ;
           owl:equivalentClass [ owl:intersectionOf ( :object_32
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty :hasImproperFunction ;
                                                        owl:hasValue :object_400
                                                      ]
                                                    ) ;
                                 rdf:type owl:Class
                               ] ;
           rdfs:subClassOf :object_32 ;
           :isPartOfPartition :object_702 ;
           rdfs:comment "Miejscowość właściwa jest taką miejscowością, której funkcja podstawowa nie polega na byciu miejscem zamieszkania."@pl ;
           rdfs:label "improper dwelling"@en ,
                      "niewłaściwa miejscowość"@pl .


###  http://purl.org/ontohgis#object_200
:object_200 rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Restriction ;
                                  owl:onProperty oc:isManifestationOf ;
                                  owl:someValuesFrom <http://purl.org/ontohgis/settlement_unit1001>
                                ] ;
            rdfs:subClassOf oc:VariableEntityManifestation ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty <http://purl.org/ontohgis/hasBoundary> ;
                              owl:someValuesFrom <http://purl.org/ontohgis/settlement_unit2000>
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty <http://purl.org/ontohgis/hasBoundary> ;
                              owl:allValuesFrom <http://purl.org/ontohgis/settlement_unit2000>
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty <http://www.opengis.net/ont/geosparql#hasGeometry> ;
                              owl:cardinality "1"^^xsd:nonNegativeInteger
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty oc:hasBeginning ;
                              owl:someValuesFrom xsd:dateTime
                            ] ;
            owl:disjointWith :object_40 ;
            rdfs:label "administrative unit manifestation"@en ,
                       "manifestacja jednostki podziału adminstracyjnego"@pl .


###  http://purl.org/ontohgis#object_201
:object_201 rdf:type owl:Class ;
            rdfs:subClassOf :object_203 ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :administersOver ;
                              owl:someValuesFrom :object_205
                            ] ;
            rdfs:label "manifestacja organu administracji religijnej"@pl .


###  http://purl.org/ontohgis#object_202
:object_202 rdf:type owl:Class ;
            rdfs:subClassOf :object_203 ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :administersOver ;
                              owl:someValuesFrom :object_206
                            ] ;
            rdfs:label "manifestacja organu administracji świeckiej"@pl .


###  http://purl.org/ontohgis#object_203
:object_203 rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Class ;
                                  owl:unionOf ( :object_201
                                                :object_202
                                              )
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty oc:isManifestationOf ;
                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onClass <http://purl.org/ontohgis/object_3005>
                                ] ;
            rdfs:subClassOf :object_200 ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :administersOver ;
                              owl:someValuesFrom owl:Thing
                            ] ;
            owl:disjointWith :object_204 ;
            rdfs:label "manifestacja organu administracji"@pl .


###  http://purl.org/ontohgis#object_204
:object_204 rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Class ;
                                  owl:unionOf ( :object_205
                                                :object_206
                                              )
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty oc:isManifestationOf ;
                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onClass <http://purl.org/ontohgis/object_3006>
                                ] ;
            rdfs:subClassOf :object_200 ;
            rdfs:label "manifestacja obszaru administracyjnego"@pl .


###  http://purl.org/ontohgis#object_205
:object_205 rdf:type owl:Class ;
            rdfs:subClassOf :object_204 ;
            rdfs:label "manifestacja obszaru administracji religijnej"@pl .


###  http://purl.org/ontohgis#object_206
:object_206 rdf:type owl:Class ;
            rdfs:subClassOf :object_204 ;
            rdfs:label "manifestacja obszaru administracji świeckiej"@pl .


###  http://purl.org/ontohgis#object_23
:object_23 rdf:type owl:Class ;
           owl:equivalentClass [ owl:intersectionOf ( <http://purl.org/ontohgis/object_1>
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty :hasProperFunction ;
                                                        owl:hasValue :object_401
                                                      ]
                                                    ) ;
                                 rdf:type owl:Class
                               ] ;
           rdfs:subClassOf <http://purl.org/ontohgis/object_1> ;
           rdfs:isDefinedBy "Budynek jest to przedmiot trwający w czasie, który został wytworzony lub wykorzystany w celu odgraniczenia pewnej przestrzeni (“wewnątrz\") od innej przestrzeni (“zewnątrz”)."@pl ;
           rdfs:label "budynek"@pl ,
                      "building"@en ;
           rdfs:seeAlso "http://encyklopedia.pwn.pl/szukaj/budynek.html" ,
                        "http://gov.genealogy.net/types.owl#17"^^xsd:anyURI .


###  http://purl.org/ontohgis#object_30
:object_30 rdf:type owl:Class ;
           rdfs:subClassOf oc:VariableEntity ,
                           obo:BFO_0000029 ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty oc:hasManifestation ;
                             owl:someValuesFrom :object_32
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :hasFunction ;
                             owl:hasValue :object_400
                           ] ;
           owl:disjointWith <http://purl.org/ontohgis/settlement_unit1001> ;
           rdfs:isDefinedBy """Jednostka osadnicza:
1. obszar posiadający nazwę własną i
2. obszar, który jest lub był w przeszłości miejscowością"""@pl ;
           rdfs:label "human settlement"@en ,
                      "jednostka osadnicza"@pl ;
           rdfs:seeAlso "http://encyklopedia.pwn.pl/haslo/jednostka-osadnicza;3917491.html"^^xsd:anyURI .


###  http://purl.org/ontohgis#object_3010
:object_3010 rdf:type owl:Class ;
